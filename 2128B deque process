https://codeforces.com/contest/2128/problem/B


#include <bits/stdc++.h>
using namespace std;
 
 
int main(){
    ios:: sync_with_stdio(false);
    cin.tie(nullptr);
    
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        
        vector<int>p(n);
        
        for(int i=0;i<n;++i){
            cin>>p[i];
          
        }
        
        string res=" ";
        int l = 0, r = n - 1;
        
        for(int i=0;i<n;++i){
            if(i%2==0 && p[l]>=p[r]){
            res.push_back('L');
            l++;
            }
            else  if(i%2==1 && p[l]<=p[r]){
            res.push_back('L');
            l++;
            }
            else{
                res.push_back('R');
            r--;
                
            }
            
        }
        cout << res << '\n';
 
        
       
    }
}
